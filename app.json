{
  "expo": {
    "name": "Your Bar",
    "slug": "yourbar",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "yourbar",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "enableProguardInRelease": true,
      "enableShrinkResourcesInRelease": true,
      "adaptiveIcon": {
        "backgroundColor": "#4DABF7",
        "foregroundImage": "./assets/images/icon.png",
        "monochromeImage": "./assets/images/icon-mono.png"
      },
      "package": "com.iamyurkas.yourbar",
      "versionCode": 4,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#4DABF7",
          "dark": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#4DABF7"
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "your-bar"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "88734f8d-dde2-4143-a6b3-e0e161dbb06e"
      }
    }
  }
}
